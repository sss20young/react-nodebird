{
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "airbnb"
    ],
    "plugins": [
        "import",
        "react-hooks"
    ],
    "rules": {
        "no-underscore-dangle": "off"
    }
}